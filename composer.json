{
	"name": "themeplate/image",
	"description": "ThemePlate image handler",
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Gene Alyson Fortunado Torcende",
			"email": "genealyson.torcende@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		},
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"require": {
		"php": "^7.4|^8.2",
		"intervention/image": "^2.7|^3.4"
	},
	"require-dev": {
		"themeplate/tester": "^2.1"
	},
	"suggest": {
		"themeplate/process": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"ThemePlate\\Image\\": "src"
		},
		"files": [
			"Image.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	}
}
